<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="408dp"
        android:layout_height="273dp"
        android:scaleX="1.2"
        app:layout_constraintBottom_toBottomOf="null"
        app:layout_constraintEnd_toEndOf="null"
        app:layout_constraintStart_toStartOf="null"
        app:layout_constraintTop_toTopOf="null"
        app:srcCompat="@android:drawable/editbox_dropdown_light_frame" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Event Title"
        android:textSize="26sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="29dp"
        app:layout_constraintTop_toTopOf="null"
        app:layout_constraintBottom_toBottomOf="null"
        app:layout_constraintStart_toStartOf="null"
        app:layout_constraintEnd_toEndOf="null"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Event Description"
        android:textSize="14sp"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="64dp"
        app:layout_constraintTop_toTopOf="null"
        app:layout_constraintBottom_toBottomOf="null"
        app:layout_constraintStart_toStartOf="null"
        app:layout_constraintEnd_toEndOf="null"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ENTRIES"
        app:layout_constraintTop_toTopOf="null"
        app:layout_constraintBottom_toBottomOf="null"
        app:layout_constraintStart_toStartOf="null"
        app:layout_constraintEnd_toEndOf="null"
        tools:layout_editor_absoluteX="178dp"
        tools:layout_editor_absoluteY="100dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="123"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="null"
        app:layout_constraintBottom_toBottomOf="null"
        app:layout_constraintStart_toStartOf="null"
        app:layout_constraintEnd_toEndOf="null"
        tools:layout_editor_absoluteX="188dp"
        tools:layout_editor_absoluteY="131dp" />

    <Button
        android:id="@+id/button1"
        android:layout_width="151dp"
        android:layout_height="47dp"
        android:text="VIEW ATTENDEES"
        android:textSize="11sp"
        android:backgroundTint="#00B6AA"
        app:layout_constraintTop_toTopOf="null"
        app:layout_constraintBottom_toBottomOf="null"
        app:layout_constraintStart_toStartOf="null"
        app:layout_constraintEnd_toEndOf="null"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="179dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="151dp"
        android:layout_height="47dp"
        android:text="EVENT DETAILS"
        android:textSize="11sp"
        android:backgroundTint="#00B6AA"
        app:layout_constraintTop_toTopOf="null"
        app:layout_constraintBottom_toBottomOf="null"
        app:layout_constraintStart_toStartOf="null"
        app:layout_constraintEnd_toEndOf="null"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="179dp" />

</androidx.constraintlayout.widget.ConstraintLayout>